{
    "manifest_version": 3,
    "name": "Vault Extension",
    "version": "1.0",
    "description": "Save credentials to your vault.",
    "permissions": ["activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  